{
  "openapi":"3.0.0",
  "info":{
    "title":"API pizzaria",
    "description":"API completa para um sistema de pizzaria",
    "contact":{
      "name":"Jana Costa",
      "email":"jana@email.com"
    },
    "version":"1.0.0"
  },
  "servers":[
    {
      "url":"http://localhost:3006/",
      "description":"Api de teste local"
    }
  ],
  "paths":{
    "/users":{
      "get":{
        "summary":"Lista todos os usuários da API",
        "description":"Rota responsável por listar todos os usuários da api",
        "tags":["Usuário"],
        "response":{
          "200":{
            "description":"OK!"
          },
          "401":{
            "description":"Unauthorized: Erro de autenticação"
          },
          "404":{
            "description":"Not Found: Nenhum usuário encontrado"
          },
          "500":{
            "description":"Erro interno do servidor"
          }
        }
      }
    },
    "/user/":{
      "get":{
        "summary":"Lista um usuário especifico por ID",
        "description":"Rota responsável por localizar um usuário especifico por ID",
        "tags":["Usuário"]
      }
    }
  }
}